{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -- -p 3001",
    "lint-staged-files": "git diff --name-only --cached | grep -E | xargs eslint --fix",
    "lint-changed-files": "git diff --name-only --cached | grep -E | xargs eslint --fix",
    "build": "set Browser_Source_Maps=false && next build",
    "build:test": "set Browser_Source_Maps=false && next build",
    "build:static": "next build && next export -o build",
    "build:development": "set Browser_Source_Maps=true&& next build",
    "build-storybook": "build-storybook -o docs-build",
    "lint": "eslint src --no-cache",
    "fix:lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\" --config ./.prettierrc",
    "start": "set NODE_OPTIONS='--max-http-header-size=24576' && next start",
    "storybook": "start-storybook -p 6006",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:noWatch": "npm run test -- --watchAll=false",
    "test:ci": "jest --watchAll=false --reporters=default --reporters=jest-junit"
  },
